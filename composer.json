{
    "name": "gemorroj/logio",
    "description": "log parser",
    "keywords": ["php", "php-fpm", "mysql", "apache", "nginx", "log", "logger"],
    "type": "library",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/config": "^4.2|^5.0",
        "symfony/yaml": "^4.0|^5.0",
        "mvar/log-parser": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Logio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Logio\\Tests\\": "tests/"
        }
    }
}
