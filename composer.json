{
    "name": "gemorroj/logio",
    "description": "log parser",
    "keywords": ["php", "php-fpm", "mysql", "apache", "nginx", "log", "logger"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.4||^7.0",
        "symfony/yaml": "^6.4||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.15",
        "friendsofphp/php-cs-fixer": "^3.73.1"
    },
    "autoload": {
        "psr-4": {
            "Logio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Logio\\Tests\\": "tests/"
        }
    }
}
